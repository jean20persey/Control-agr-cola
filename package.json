{
  "name": "control-agricola-api",
  "version": "1.0.0",
  "description": "Sistema de gestión y análisis de producción agrícola",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python manage.py runserver",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createsuperuser": "cd backend && python manage.py createsuperuser"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["django", "react", "typescript", "agriculture", "analytics", "postgresql"],
  "author": "Control Agrícola Team",
  "license": "MIT"
}
